<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AdminDashboardController">
    <TableView fx:id="booksTable" layoutY="53.0" prefHeight="547.0" prefWidth="800">
        <columns>
            <TableColumn fx:id="titleColumn" text="Title" />
            <TableColumn fx:id="authorColumn" text="Author" />
            <TableColumn fx:id="priceColumn" text="Price" />
            <TableColumn fx:id="quantityColumn" text="Quantity" />
            <TableColumn fx:id="soldColumn" text="Sold" />
        </columns>
    </TableView>
   <Label layoutX="217.0" layoutY="14.0" text="Welcome Admin">
      <font>
         <Font name="Arial Bold" size="17.0" />
      </font>
   </Label>
   <Button fx:id="btnlogout" layoutX="688.0" layoutY="7.0" mnemonicParsing="false" onAction="#goToLogOut" prefHeight="34.0" prefWidth="68.0" style="-fx-background-color: black;" text="Log Out" textFill="WHITE">
      <font>
         <Font name="Arial Bold" size="13.0" />
      </font>
   </Button>
</AnchorPane>
