<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="Controllers.AdminDashboardController">
    <TableView fx:id="booksTable" prefWidth="800" prefHeight="600">
        <columns>
            <TableColumn fx:id="titleColumn" text="Title" />
            <TableColumn fx:id="authorColumn" text="Author" />
            <TableColumn fx:id="priceColumn" text="Price" />
            <TableColumn fx:id="quantityColumn" text="Quantity" />
            <TableColumn fx:id="soldColumn" text="Sold" />
        </columns>
    </TableView>
</AnchorPane>
